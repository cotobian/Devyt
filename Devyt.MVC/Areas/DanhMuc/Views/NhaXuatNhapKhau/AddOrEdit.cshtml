@model Devyt.Models.DanhMuc_NhaXuatNhapKhau
@{
    Layout = null;
}

@using (Html.BeginForm("AddOrEdit", "NhaXuatNhapKhau", FormMethod.Post, new { onsubmit = "return SubmitForm(this)" }))
{
    @Html.HiddenFor(model => model.Id)
    <div class="form-group">
        <label class="control-label">Tên Nhà Xuất nhập khẩu</label>
        @Html.EditorFor(model => model.TenNhaXNK, new { htmlAttributes = new { @class = "form-control" } })
    </div>
    <div class="form-group">
        <input type="submit" value="Cập nhật" class="btn btn-primary" />
    </div>
}